{
    "$schema":"https://raw.githubusercontent.com/GeneralElectric/TrueConnect-Link/master/docs/LinkConfigurationSchema.json#",
    "runasservice": false,
    "clientid": "xxxxxxxxxxxxxxxxxx",
    "endpoint": "xxxxxxxxxxxxxxxxxx",
    "secret": "xxxxxxxxxxxxxxxxxx",
    "tokenurl": "xxxxxxxxxxxxxxxxxx",
    "concurrentuploads": 1,
    "targets": [
        {
            "name": "RawLeapEngineData",
            "active": true,
            "dataformat": "CEOD",
            "datatype": "raw",
            "location": "xxxxxxxxxxxxxxxxxx",
            "tenant": "xxxxxxxxxxxxxxxxxx",
            "match": ".{12}FFD.{49}$",
            "pollinterval": 360,
            "recursive": false,
            "pathencodedmetadatatags": [
                {
                    "tag": "TailNo",
                    "match": "([^_]{1,2}_{0,1}[^_]{1,5})_*-.{3}-FFD-\\d{8}-\\d{6}-.{2}-[^_]{0,10}_*-[^_]{0,8}_*-\\d{6}[AB]\\d{2}$"
                },
                {
                    "tag": "EngPos",
                    "match": "[^_]{1,2}_{0,1}[^_]{1,5}_*-(.{3})-FFD-\\d{8}-\\d{6}-.{2}-[^_]{0,10}_*-[^_]{0,8}_*-\\d{6}[AB]\\d{2}$"
                },
                {
                    "tag": "FlightDate",
                    "match": "[^_]{1,2}_{0,1}[^_]{1,5}_*-.{3}-FFD-(\\d{8}-\\d{6})-.{2}-[^_]{0,10}_*-[^_]{0,8}_*-\\d{6}[AB]\\d{2}$"
                },
                {
                    "tag": "IATA",
                    "match": "[^_]{1,2}_{0,1}[^_]{1,5}_*-.{3}-FFD-\\d{8}-\\d{6}-(.{2})-[^_]{0,10}_*-[^_]{0,8}_*-\\d{6}[AB]\\d{2}$"
                },
                {
                    "tag": "FlightNo",
                    "match": "[^_]{1,2}_{0,1}[^_]{1,5}_*-.{3}-FFD-\\d{8}-\\d{6}-.{2}-([^_]{0,10})_*-[^_]{0,8}_*-\\d{6}[AB]\\d{2}$"
                },
                {
                    "tag": "CityPair",
                    "match": "[^_]{1,2}_{0,1}[^_]{1,5}_*-.{3}-FFD-\\d{8}-\\d{6}-.{2}-[^_]{0,10}_*-([^_]{0,8})_*-\\d{6}[AB]\\d{2}$"
                },
                {
                    "tag": "ESN",
                    "match": "[^_]{1,2}_{0,1}[^_]{1,5}_*-.{3}-FFD-\\d{8}-\\d{6}-.{2}-[^_]{0,10}_*-[^_]{0,8}_*-(\\d{6})[AB]\\d{2}$"
                },
                {
                    "tag": "Channel",
                    "match": "[^_]{1,2}_{0,1}[^_]{1,5}_*-.{3}-FFD-\\d{8}-\\d{6}-.{2}-[^_]{0,10}_*-[^_]{0,8}_*-\\d{6}([AB])\\d{2}$"
                },
                {
                    "tag": "Version",
                    "match": "[^_]{1,2}_{0,1}[^_]{1,5}_*-.{3}-FFD-\\d{8}-\\d{6}-.{2}-[^_]{0,10}_*-[^_]{0,8}_*-\\d{6}[AB]\\(d{2})$"
                }
            ],
            "statictags": [
                {
                    "tag": "ICAO",
                    "value": "xxxxxxxxxxxxxxxxxx"
                },
                {
                    "tag": "Opertator",
                    "value": "xxxxxxxxxxxxxxxxxx"
                }
            ]
        },
        {            
            "name": "ZippedLeapEngineData",
            "active": true,
            "dataformat": "CEOD",
            "datatype": "zip",
            "location": "xxxxxxxxxxxxxxxxxx",
            "tenant": "xxxxxxxxxxxxxxxxxx",
            "match": ".{12}FFD.*\\.zip$",
            "pollinterval": 360,
            "recursive": false,
            "pathencodedmetadatatags": [
                {
                    "tag": "IATA",
                    "match": "(.{2})[^_]{1,7}_*.{3}FFD_FFD_\\d{14}\\+\\d{4}-\\[.*\\]\\.zip$"
                },
                {
                    "tag": "TailNo",
                    "match": ".{2}([^_]{1,7})_*.{3}FFD_FFD_\\d{14}\\+\\d{4}-\\[.*\\]\\.zip$"
                },
                {
                    "tag": "EngPos",
                    "match": ".{2}([^_]{1,7})_*(.{3})FFD_FFD_\\d{14}\\+\\d{4}-\\[.*\\]\\.zip$"
                },
                {
                    "tag": "FlightDate",
                    "match": ".{2}([^_]{1,7})_*.{3}FFD_FFD_(\\d{14}\\+\\d{4})-\\[.*\\]\\.zip$"
                },
                {
                    "tag": "BoeTag",
                    "match": ".{2}([^_]{1,7})_*.{3}FFD_FFD_\\d{14}\\+\\d{4}-\\[(.*)\\]\\.zip$"
                }
            ],
            "statictags": [
                {
                    "tag": "ICAO",
                    "value": "xxxxxxxxxxxxxxxxxx"
                },
                {
                    "tag": "Opertator",
                    "value": "xxxxxxxxxxxxxxxxxx"
                }
            ]
        },
        {
            "name":"FindTestFiles",
            "active":true,
            "tenant":"xxxxxxxxxxxxxxxxxx",
            "location": "xxxxxxxxxxxxxxxxxx",
            "recursive": false,
            "match": ".*FFD$",
            "datatype": "CEOD",
            "dataformat": "raw",
            "pathencodedmetadatatags":[
                {
                    "tag": "FlightDate",
                    "match": "(\\d{6}-\\d{6})-[^-]{10}-[^-]{8}-.{9}-[^-]-[^-]{6}-[^_]{4}FFD$"
                },
                {
                    "tag": "FlightNo",
                    "match": "[^_]{6}-[^_]{6}-([^_]{0,10})_*-[^-]{8}-.{9}-[^-]-[^-]{6}-[^_]{4}FFD$"
                },
                {
                    "tag": "CityPair",
                    "match": "[^-]{6}-[^-]{6}-[^-]{10}-([^_]{0,8})_*-.{9}-[^-]-[^-]{6}-[^_]{4}FFD$"
                },
                {
                    "tag": "IATA",
                    "match": "[^-]{6}-[^-]{6}-[^-]{10}-[^-]{8}-([^_-]{2}).{7}-[^-]-[^-]{6}-[^_]{4}FFD$"
                },
                {
                    "tag": "TailNo",
                    "match": "[^-]{6}-[^-]{6}-[^-]{10}-[^-]{8}-[^-]{2}_{0,1}([^_]{1,6}[^-_]{0,1})-[^-]-[^-]{6}-[^_]{4}FFD$"
                },
                {
                    "tag": "EngPos",
                    "match": "[^-]{6}-[^-]{6}-[^-]{10}-[^-]{8}-.{9}-(\\d)-[^-]{6}-[^_]{4}FFD$"
                },
                {
                    "tag": "ESN",
                    "match": "[^-]{6}-[^-]{6}-[^-]{10}-[^-]{8}-.{9}-[^-]-(\\d{6})-[^_]{4}FFD$"
                },
                {
                    "tag": "SPN",
                    "match": "[^-]{6}-[^-]{6}-[^-]{10}-[^-]{8}-.{9}-[^-]-[^-]{6}-([^_]{4})FFD$"
                }
            ],
            "statictags": [
                {
                    "tag": "ICAO",
                    "value": "xxxxxxxxxxxxxxxxxx"
                },
                {
                    "tag": "Opertator",
                    "value": "xxxxxxxxxxxxxxxxxx"
                }
            ],
            "pollinterval": 300
        }
    ]    
}
